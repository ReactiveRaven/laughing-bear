<?php

namespace RRaven\Bundle\GitpanelBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use RRaven\Bundle\GitpanelBundle\Annotations\Api;

use \DateTime;

/**
 * @ORM\Entity
 */
class GithubUser
{
	
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;
    
    /**
     * @ORM\Column(type="integer")
		 * @Api\Map(keys={"id"})
     */
    protected $github_id;
    
    /**
     * @ORM\Column(type="string", length=255)
		 * @Api\Map(keys={"login"})
     */
    protected $login;
    
    /**
     * @ORM\Column(type="string", length=255)
		 * @Api\Map(keys={"avatar_url"})
     */
    protected $avatar_url;
    
    /**
     * @ORM\Column(type="string", length=32, nullable=true)
		 * @Api\Map(keys={"gravatar_id"})
     */
    protected $gravatar_id;
    
    /**
     * @ORM\Column(type="string", length=255)
		 * @Api\Map(keys={"url"})
     */
    protected $url;
    
    /**
     * @ORM\Column(type="string", length=255)
		 * @Api\Map(keys={"name"})
     */
    protected $name;
    
    /**
     * @ORM\Column(type="string", length=255, nullable=true)
		 * @Api\Map(keys={"company"})
     */
    protected $company;
    
    /**
     * @ORM\Column(type="string", length=255, nullable=true)
		 * @Api\Map(keys={"blog_url", "blog"})
     */
    protected $blog;
    
    /**
     * @ORM\Column(type="string", length=2000, nullable=true)
		 * @Api\Map(keys={"html_url"})
     */
    protected $html_url;
    
    /**
     * @ORM\Column(type="string", length=255, nullable=true)
		 * @Api\Map(keys={"location"})
     */
    protected $location;
    
    /**
     * @ORM\Column(type="string", length=255, nullable=true)
		 * @Api\Map(keys={"email"})
     */
    protected $email;
    
    /**
     * @ORM\Column(type="boolean", nullable=true)
		 * @Api\Map(keys={"hireable"})
     */
    protected $hireable;
    
    /**
     * @ORM\Column(type="text", nullable=true)
		 * @Api\Map(keys={"bio"})
     */
    protected $bio;
    
    /**
     * @ORM\Column(type="datetime")
		 * @Api\Map(keys={"created_at"})
     */
    protected $created_at;
    
    /**
     * @ORM\Column(type="integer")
		 * @Api\Map(keys={"public_repos"})
     */
    protected $public_repos;
    
    /**
     * @ORM\Column(type="integer")
		 * @Api\Map(keys={"followers"})
     */
    protected $followers;
    
    /**
     * @ORM\Column(type="integer")
		 * @Api\Map(keys={"following"})
     */
    protected $following;
    
    /**
     * @ORM\Column(type="integer")
		 * @Api\Map(keys={"public_gists"})
     */
    protected $public_gists;
    
    /**
     * @ORM\Column(type="string", length=255)
		 * @Api\Map(keys={"type"})
     */
    protected $type;

    /**
     * @ORM\OneToOne(targetEntity="GithubAccessToken", fetch="LAZY", inversedBy="user")
     */
    protected $accessToken;
		
		/**
		 * @ORM\OneToMany(targetEntity="GithubRepository", fetch="LAZY", mappedBy="user")
		 */
		protected $repositories;
    
    /**
     * Manufacture an instance
     * @return \RRaven\Bundle\GitpanelBundle\Entity\GithubUser
     */
    public static function manufacture()
    {
      return new GithubUser();
    }
    
    public function __construct()
    {
        $this->repositories = new \Doctrine\Common\Collections\ArrayCollection();
    }
    
    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set github_id
     *
     * @param integer $githubId
     * @return GithubUser
     */
    public function setGithubId($githubId)
    {
        $this->github_id = $githubId;
        return $this;
    }

    /**
     * Get github_id
     *
     * @return integer 
     */
    public function getGithubId()
    {
        return $this->github_id;
    }

    /**
     * Set login
     *
     * @param string $login
     * @return GithubUser
     */
    public function setLogin($login)
    {
        $this->login = $login;
        return $this;
    }

    /**
     * Get login
     *
     * @return string 
     */
    public function getLogin()
    {
        return $this->login;
    }

    /**
     * Set avatar_url
     *
     * @param string $avatarUrl
     * @return GithubUser
     */
    public function setAvatarUrl($avatarUrl)
    {
        $this->avatar_url = $avatarUrl;
        return $this;
    }

    /**
     * Get avatar_url
     *
     * @return string 
     */
    public function getAvatarUrl()
    {
        return $this->avatar_url;
    }

    /**
     * Set gravatar_id
     *
     * @param string $gravatarId
     * @return GithubUser
     */
    public function setGravatarId($gravatarId)
    {
        $this->gravatar_id = $gravatarId;
        return $this;
    }

    /**
     * Get gravatar_id
     *
     * @return string 
     */
    public function getGravatarId()
    {
        return $this->gravatar_id;
    }

    /**
     * Set url
     *
     * @param string $url
     * @return GithubUser
     */
    public function setUrl($url)
    {
        $this->url = $url;
        return $this;
    }

    /**
     * Get url
     *
     * @return string 
     */
    public function getUrl()
    {
        return $this->url;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return GithubUser
     */
    public function setName($name)
    {
        $this->name = $name;
        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set company
     *
     * @param string $company
     * @return GithubUser
     */
    public function setCompany($company)
    {
        $this->company = $company;
        return $this;
    }

    /**
     * Get company
     *
     * @return string 
     */
    public function getCompany()
    {
        return $this->company;
    }

    /**
     * Set blog
     *
     * @param string $blog
     * @return GithubUser
     */
    public function setBlog($blog)
    {
        $this->blog = $blog;
        return $this;
    }

    /**
     * Get blog
     *
     * @return string 
     */
    public function getBlog()
    {
        return $this->blog;
    }

    /**
     * Set html_url
     *
     * @param string $htmlUrl
     * @return GithubUser
     */
    public function setHtmlUrl($htmlUrl)
    {
        $this->html_url = $htmlUrl;
        return $this;
    }

    /**
     * Get html_url
     *
     * @return string 
     */
    public function getHtmlUrl()
    {
        return $this->html_url;
    }

    /**
     * Set location
     *
     * @param string $location
     * @return GithubUser
     */
    public function setLocation($location)
    {
        $this->location = $location;
        return $this;
    }

    /**
     * Get location
     *
     * @return string 
     */
    public function getLocation()
    {
        return $this->location;
    }

    /**
     * Set email
     *
     * @param string $email
     * @return GithubUser
     */
    public function setEmail($email)
    {
        $this->email = $email;
        return $this;
    }

    /**
     * Get email
     *
     * @return string 
     */
    public function getEmail()
    {
        return $this->email;
    }

    /**
     * Set hireable
     *
     * @param boolean $hireable
     * @return GithubUser
     */
    public function setHireable($hireable)
    {
        $this->hireable = $hireable;
        return $this;
    }

    /**
     * Get hireable
     *
     * @return boolean 
     */
    public function getHireable()
    {
        return $this->hireable;
    }

    /**
     * Set bio
     *
     * @param text $bio
     * @return GithubUser
     */
    public function setBio($bio)
    {
        $this->bio = $bio;
        return $this;
    }

    /**
     * Get bio
     *
     * @return text 
     */
    public function getBio()
    {
        return $this->bio;
    }

    /**
     * Set created_at
     *
     * @param datetime $createdAt
     * @return GithubUser
     */
    public function setCreatedAt($createdAt)
    {
        $this->created_at = $createdAt;
        return $this;
    }

    /**
     * Get created_at
     *
     * @return datetime 
     */
    public function getCreatedAt()
    {
        return $this->created_at;
    }

    /**
     * Set public_repos
     *
     * @param integer $publicRepos
     * @return GithubUser
     */
    public function setPublicRepos($publicRepos)
    {
        $this->public_repos = $publicRepos;
        return $this;
    }

    /**
     * Get public_repos
     *
     * @return integer 
     */
    public function getPublicRepos()
    {
        return $this->public_repos;
    }

    /**
     * Set followers
     *
     * @param integer $followers
     * @return GithubUser
     */
    public function setFollowers($followers)
    {
        $this->followers = $followers;
        return $this;
    }

    /**
     * Get followers
     *
     * @return integer 
     */
    public function getFollowers()
    {
        return $this->followers;
    }

    /**
     * Set following
     *
     * @param integer $following
     * @return GithubUser
     */
    public function setFollowing($following)
    {
        $this->following = $following;
        return $this;
    }

    /**
     * Get following
     *
     * @return integer 
     */
    public function getFollowing()
    {
        return $this->following;
    }

    /**
     * Set public_gists
     *
     * @param integer $publicGists
     * @return GithubUser
     */
    public function setPublicGists($publicGists)
    {
        $this->public_gists = $publicGists;
        return $this;
    }

    /**
     * Get public_gists
     *
     * @return integer 
     */
    public function getPublicGists()
    {
        return $this->public_gists;
    }

    /**
     * Set type
     *
     * @param string $type
     * @return GithubUser
     */
    public function setType($type)
    {
        $this->type = $type;
        return $this;
    }

    /**
     * Get type
     *
     * @return string 
     */
    public function getType()
    {
        return $this->type;
    }

    /**
     * Set accessToken
     *
     * @param RRaven\Bundle\GitpanelBundle\Entity\GithubAccessToken $accessToken
     * @return GithubUser
     */
    public function setAccessToken(\RRaven\Bundle\GitpanelBundle\Entity\GithubAccessToken $accessToken = null)
    {
        $this->accessToken = $accessToken;
        return $this;
    }

    /**
     * Get accessToken
     *
     * @return RRaven\Bundle\GitpanelBundle\Entity\GithubAccessToken 
     */
    public function getAccessToken()
    {
        return $this->accessToken;
    }

    /**
     * Add repositories
     *
     * @param RRaven\Bundle\GitpanelBundle\Entity\GithubRepository $repositories
     * @return GithubUser
     */
    public function addRepositorie(\RRaven\Bundle\GitpanelBundle\Entity\GithubRepository $repositories)
    {
        $this->repositories[] = $repositories;
        return $this;
    }

    /**
     * Remove repositories
     *
     * @param RRaven\Bundle\GitpanelBundle\Entity\GithubRepository $repositories
     */
    public function removeRepositorie(\RRaven\Bundle\GitpanelBundle\Entity\GithubRepository $repositories)
    {
        $this->repositories->removeElement($repositories);
    }

    /**
     * Get repositories
     *
     * @return Doctrine\Common\Collections\Collection 
     */
    public function getRepositories()
    {
        return $this->repositories;
    }
}